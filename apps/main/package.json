    "electron": "^35.0.3",
    "electron-browser-use": "^0.0.3",
    "electron-is-dev": "^3.0.1",
    "exceljs": "^4.4.0",
    "install": "^0.13.0",
    "js-yaml": "^4.1.0",
    "mammoth": "^1.9.0",
    "marked": "^15.0.7",
    "pdfjs-dist": "^5.0.375",
    "pptx-parser": "^1.1.7-beta.9",
    "pptxgenjs": "^3.12.0",
    "prisma": "^6.5.0",
