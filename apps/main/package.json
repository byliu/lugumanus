{
  "name": "@lugu-manus/main",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/main.js",
  "nx": {
    "$schema": "../../node_modules/nx/schemas/nx-schema.json",
    "targets": {
      "dev": {
        "executor": "nx:run-commands",
        "defaultConfiguration": "development",
        "options": {
          "cwd": "apps/main",
          "commands": [
            {
              "command": "bun run scripts/bootstrap.tsx"
            }
          ]
        }
      }
    }
  },
  "dependencies": {
    "@prisma/client": "6.5.0",
    "@trpc/server": "^11.0.0",
    "electron": "^35.0.3",
    "electron-is-dev": "^3.0.1",
    "prisma": "^6.5.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.1",
    "bun-types": "^1.2.5",
    "nodemon": "^3.0.3"
  }
}
